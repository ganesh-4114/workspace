
package ramesh;

public class Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String arr[][] = {{"ganesh","dandamudi"},{"sheshank","pendli"}};
		for (int i=0;i <arr.length; i++) {
			for (int j=0; j<arr.length; j++) {
				System.out.println(arr[i][j]);
			}
		}
	}
}
