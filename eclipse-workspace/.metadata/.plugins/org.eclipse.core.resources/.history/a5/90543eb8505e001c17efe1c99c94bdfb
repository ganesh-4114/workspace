package ramesh;
class Parent {
	 void dev() {
		 String role;
		 int experience;
		System.out.println("Developer");
	}
}

class Hybrid extends Parent {
	void hi() {
		System.out.println("ganesh");
	}
	void newjoin() {
		System.out.println("Yes, ganesh is newly joined");
	}
}
 class Child {
	public static void main(String args[]) {
       Hybrid emp = new Hybrid();
		emp.dev();
		emp.hi();
		emp.newjoin();
	}

}

