
package ramesh;

public class Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String arr[][] = {{"ganesh","dandamudi"},{"sheshank","pendli"},{"ramesh","polla"}};
		for (int i=0;i<2; i++) {
			for (int j=0; j<2; j++) {
				System.out.print(arr[i][j] + " ");
			}
			System.out.println();
		}
	}
}
